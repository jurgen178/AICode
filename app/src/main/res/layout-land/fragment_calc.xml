<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

  <androidx.constraintlayout.widget.ConstraintLayout
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      >

    <TextView
        android:id="@+id/calc_percentChange"
        android:text="∆%"
        app:layout_constraintBottom_toTopOf="@id/guidelineF1"
        app:layout_constraintEnd_toStartOf="@id/guidelineX5"
        app:layout_constraintStart_toEndOf="@id/guidelineX4"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF2"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_CopyToClipboard"
        android:text="@string/calc_copy"
        app:layout_constraintBottom_toTopOf="@id/guidelineF1"
        app:layout_constraintEnd_toStartOf="@id/guidelineX2"
        app:layout_constraintStart_toEndOf="@id/guidelineX1"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF2"
        style="@style/calc_extrakeys_land_style"
        />

    <TextView
        android:id="@+id/calc_CopyFromClipboard"
        android:text="@string/calc_paste"
        app:layout_constraintBottom_toTopOf="@id/guidelineF1"
        app:layout_constraintEnd_toStartOf="@id/guidelineX3"
        app:layout_constraintStart_toEndOf="@id/guidelineX2"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF2"
        style="@style/calc_extrakeys_land_style"
        />

    <TextView
        android:id="@+id/calc_percent"
        android:text="%"
        app:layout_constraintBottom_toTopOf="@id/guidelineF1"
        app:layout_constraintEnd_toStartOf="@id/guidelineX4"
        app:layout_constraintStart_toEndOf="@id/guidelineX3"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF2"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_swap"
        android:text="SWAP"
        app:layout_constraintBottom_toTopOf="@id/guidelineF2"
        app:layout_constraintEnd_toStartOf="@id/guidelineX5"
        app:layout_constraintStart_toEndOf="@id/guidelineX4"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF3"
        style="@style/calc_extrakeys_land_style"
        />

    <TextView
        android:id="@+id/calc_over"
        android:text="OVER"
        app:layout_constraintBottom_toTopOf="@id/guidelineF2"
        app:layout_constraintEnd_toStartOf="@id/guidelineX4"
        app:layout_constraintStart_toEndOf="@id/guidelineX3"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF3"
        style="@style/calc_extrakeys_land_style"
        />

    <TextView
        android:id="@+id/calc_SQRT"
        android:text="√x"
        app:layout_constraintBottom_toTopOf="@id/guidelineEnter"
        app:layout_constraintEnd_toStartOf="@id/guidelineX2"
        app:layout_constraintStart_toEndOf="@id/guidelineX1"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF1"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_SQ"
        android:text="x²"
        app:layout_constraintBottom_toTopOf="@id/guidelineEnter"
        app:layout_constraintEnd_toStartOf="@id/guidelineX3"
        app:layout_constraintStart_toEndOf="@id/guidelineX2"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF1"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_POW"
        android:text="x^y"
        app:layout_constraintBottom_toTopOf="@id/guidelineEnter"
        app:layout_constraintEnd_toStartOf="@id/guidelineX4"
        app:layout_constraintStart_toEndOf="@id/guidelineX3"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF1"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_INV"
        android:text="1/x"
        app:layout_constraintBottom_toTopOf="@id/guidelineEnter"
        app:layout_constraintEnd_toStartOf="@id/guidelineX5"
        app:layout_constraintStart_toEndOf="@id/guidelineX4"
        app:layout_constraintTop_toBottomOf="@+id/guidelineF1"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_enter"
        android:text="ENTER"
        app:layout_constraintBottom_toTopOf="@id/guideline789"
        app:layout_constraintEnd_toStartOf="@id/guidelineX3"
        app:layout_constraintStart_toEndOf="@id/guidelineX1"
        app:layout_constraintTop_toBottomOf="@+id/guidelineEnter"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_drop"
        android:text="\u232b"
        app:layout_constraintBottom_toTopOf="@id/guideline789"
        app:layout_constraintEnd_toStartOf="@id/guidelineX5"
        app:layout_constraintStart_toEndOf="@id/guidelineX4"
        app:layout_constraintTop_toBottomOf="@+id/guidelineEnter"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_eex"
        android:text="EEX"
        app:layout_constraintBottom_toTopOf="@id/guideline789"
        app:layout_constraintEnd_toStartOf="@id/guidelineX4"
        app:layout_constraintStart_toEndOf="@id/guidelineX3"
        app:layout_constraintTop_toBottomOf="@+id/guidelineEnter"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_7"
        android:text="7"
        app:layout_constraintBottom_toTopOf="@id/guideline456"
        app:layout_constraintEnd_toStartOf="@id/guidelineX2"
        app:layout_constraintStart_toEndOf="@id/guidelineX1"
        app:layout_constraintTop_toBottomOf="@+id/guideline789"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_8"
        android:text="8"
        app:layout_constraintBottom_toTopOf="@id/guideline456"
        app:layout_constraintEnd_toStartOf="@id/guidelineX3"
        app:layout_constraintStart_toEndOf="@id/guidelineX2"
        app:layout_constraintTop_toBottomOf="@+id/guideline789"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_9"
        android:text="9"
        app:layout_constraintBottom_toTopOf="@id/guideline456"
        app:layout_constraintEnd_toStartOf="@id/guidelineX4"
        app:layout_constraintStart_toEndOf="@id/guidelineX3"
        app:layout_constraintTop_toBottomOf="@+id/guideline789"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_4"
        android:text="4"
        app:layout_constraintBottom_toTopOf="@id/guideline123"
        app:layout_constraintEnd_toStartOf="@id/guidelineX2"
        app:layout_constraintStart_toEndOf="@id/guidelineX1"
        app:layout_constraintTop_toBottomOf="@+id/guideline456"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_5"
        android:text="5"
        app:layout_constraintBottom_toTopOf="@id/guideline123"
        app:layout_constraintEnd_toStartOf="@id/guidelineX3"
        app:layout_constraintStart_toEndOf="@id/guidelineX2"
        app:layout_constraintTop_toBottomOf="@+id/guideline456"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_6"
        android:text="6"
        app:layout_constraintBottom_toTopOf="@id/guideline123"
        app:layout_constraintEnd_toStartOf="@id/guidelineX4"
        app:layout_constraintStart_toEndOf="@id/guidelineX3"
        app:layout_constraintTop_toBottomOf="@+id/guideline456"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_1"
        android:text="1"
        app:layout_constraintBottom_toTopOf="@id/guideline0"
        app:layout_constraintEnd_toStartOf="@id/guidelineX2"
        app:layout_constraintStart_toEndOf="@id/guidelineX1"
        app:layout_constraintTop_toBottomOf="@+id/guideline123"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_2"
        android:text="2"
        app:layout_constraintBottom_toTopOf="@id/guideline0"
        app:layout_constraintEnd_toStartOf="@id/guidelineX3"
        app:layout_constraintStart_toEndOf="@id/guidelineX2"
        app:layout_constraintTop_toBottomOf="@+id/guideline123"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_3"
        android:text="3"
        app:layout_constraintBottom_toTopOf="@id/guideline0"
        app:layout_constraintEnd_toStartOf="@id/guidelineX4"
        app:layout_constraintStart_toEndOf="@id/guidelineX3"
        app:layout_constraintTop_toBottomOf="@+id/guideline123"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_0"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@id/guideline00"
        app:layout_constraintEnd_toStartOf="@id/guidelineX2"
        app:layout_constraintStart_toEndOf="@id/guidelineX1"
        app:layout_constraintTop_toBottomOf="@+id/guideline0"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_dot"
        android:text="."
        app:layout_constraintBottom_toTopOf="@id/guideline00"
        app:layout_constraintEnd_toStartOf="@id/guidelineX3"
        app:layout_constraintStart_toEndOf="@id/guidelineX2"
        app:layout_constraintTop_toBottomOf="@+id/guideline0"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_sign"
        android:text="±"
        app:layout_constraintBottom_toTopOf="@id/guideline00"
        app:layout_constraintEnd_toStartOf="@id/guidelineX4"
        app:layout_constraintStart_toEndOf="@id/guidelineX3"
        app:layout_constraintTop_toBottomOf="@+id/guideline0"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_div"
        android:text="÷"
        app:layout_constraintBottom_toTopOf="@id/guideline456"
        app:layout_constraintEnd_toStartOf="@id/guidelineX5"
        app:layout_constraintStart_toEndOf="@id/guidelineX4"
        app:layout_constraintTop_toBottomOf="@+id/guideline789"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_mult"
        android:text="*"
        app:layout_constraintBottom_toTopOf="@id/guideline123"
        app:layout_constraintEnd_toStartOf="@id/guidelineX5"
        app:layout_constraintStart_toEndOf="@id/guidelineX4"
        app:layout_constraintTop_toBottomOf="@+id/guideline456"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_sub"
        android:text="-"
        app:layout_constraintBottom_toTopOf="@id/guideline0"
        app:layout_constraintEnd_toStartOf="@id/guidelineX5"
        app:layout_constraintStart_toEndOf="@id/guidelineX4"
        app:layout_constraintTop_toBottomOf="@+id/guideline123"
        style="@style/calc_land_style"
        />

    <TextView
        android:id="@+id/calc_add"
        android:text="+"
        app:layout_constraintBottom_toTopOf="@id/guideline00"
        app:layout_constraintEnd_toStartOf="@id/guidelineX5"
        app:layout_constraintStart_toEndOf="@id/guidelineX4"
        app:layout_constraintTop_toBottomOf="@+id/guideline0"
        style="@style/calc_land_style"
        />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/guidelineIndicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="top"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineF3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.02"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineF2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.14"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineF1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.26"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineEnter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.38"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline789"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.5"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline456"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.62"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline123"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.74"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.86"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.98"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineX0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.45"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineTop1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.06"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineTop2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.16"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineTop3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.26"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineTop4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.36"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineX1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineX2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.62"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineX3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.74"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineX4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.86"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineX5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.98"
        />

    <TextView
        android:id="@+id/calc_indicator_left"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/guidelineTop1"
        app:layout_constraintBottom_toTopOf="@id/guidelineIndicator"
        style="@style/calc_indicator_style"
        />

    <TextView
        android:id="@+id/calc_indicator_depth"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/guidelineTop1"
        app:layout_constraintEnd_toStartOf="@id/guidelineTop2"
        app:layout_constraintBottom_toTopOf="@id/guidelineIndicator"
        style="@style/calc_indicator_style"
        />

    <TextView
        android:id="@+id/calc_indicator_radian"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/guidelineTop3"
        app:layout_constraintStart_toEndOf="@id/guidelineTop2"
        app:layout_constraintBottom_toTopOf="@id/guidelineIndicator"
        style="@style/calc_indicator_style"
        />

    <TextView
        android:id="@+id/calc_indicator_radix"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/guidelineTop4"
        app:layout_constraintStart_toEndOf="@id/guidelineTop3"
        app:layout_constraintBottom_toTopOf="@id/guidelineIndicator"
        style="@style/calc_indicator_style"
        />

    <TextView
        android:id="@+id/calc_indicator_shift"
        android:textStyle="bold"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/guidelineX0"
        app:layout_constraintStart_toEndOf="@id/guidelineTop4"
        app:layout_constraintBottom_toTopOf="@id/guidelineIndicator"
        style="@style/calc_indicator_style"
        />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/calclines"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="#CCC69B"
        android:padding="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/guidelineX0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guidelineIndicator"
        />

  </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
